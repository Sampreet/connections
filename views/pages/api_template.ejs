<!DOCTYPE html>
<html lang='en'>
  <head>
    <% include ../partials/head %>
    <%- include('../../apis/' + api_name) %>
  </head>
  <body onload='initialize()'>
    <header>
      <% include ../partials/header %>
    </header>

    <main>
      <div class='row'>
        <div class='col-sm-10 container'>
          <div class='well'>
            <h1><%= heading%></h1>
            <h4><%= sub_heading%></h4>
          </div>
          <div class='well'>
            <% include ../partials/report %>
          </div>
        </div>
        <div class='col-sm-2'>
          <div class='well'>
            <% include ../partials/update %>
          </div>
        </div>
      </div>
    </main>

    <footer>
      <% include ../partials/footer %>
    </footer>
  </body>
</html>
