<!DOCTYPE html>
<html lang='en'>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <header>
      <% include ../partials/header %>
    </header>

    <main>
      <div class='row'>
        <div class='col-sm-12 container'>
          <div class='well'>
            <% if(status == 200) { %>
              <h1><%= heading%></h1>
              <div id='result'>
                <p>Module Name: <%= module_name %></p>
                <p>Message: <%= message %></p>
                <p>Object: <%= JSON.stringify(object) %></p>
              </div>
            <% } else { %>
              <h1 id='result_label'>Error: </h1>
              <div id='result'>
                <p>Module Name: <%= module_name %></p>
                <p>Error Type: <%= error_type %></p>
                <p>Error Message: <%= error_msg %></p>
                <p>Error Object: <%= JSON.stringify(error_obj) %></p>
              </div>
            <% } %>
          </div>
        </div>
      </div>
    </main>

    <footer>
      <% include ../partials/footer %>
    </footer>
  </body>
</html>